          "_name": "content7",
          "_sourceTheme": "claimm4",
          "_customHTML": "<section class=\"content7\" group=\"Content\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"6\">\n       <select title=\"Cards\" name=\"cardsCount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n              <input type=\"range\" inline title=\"Card 1 Logo Count\" name=\"items1\" min=\"0\" max=\"4\" value=\"1\" step=\"1\">\n              <input type=\"range\" inline title=\"Card 2 Logo Count\" name=\"items2\" min=\"0\" max=\"4\" value=\"2\" step=\"1\" condition=\"cardsCount > 1\">\n              <input type=\"range\" inline title=\"Card 3 Logo Count\" name=\"items3\" min=\"0\" max=\"4\" value=\"3\" step=\"1\" condition=\"cardsCount > 2\">\n              <input type=\"range\" inline title=\"Card 4 Logo Count\" name=\"items4\" min=\"0\" max=\"4\" value=\"4\" step=\"1\" condition=\"cardsCount > 3\">\n              <input type=\"range\" inline title=\"Card 5 Logo Count\" name=\"items5\" min=\"0\" max=\"4\" value=\"2\" step=\"1\" condition=\"cardsCount > 4\">\n              <input type=\"range\" inline title=\"Card 6 Logo Count\" name=\"items6\" min=\"0\" max=\"4\" value=\"0\" step=\"1\" condition=\"cardsCount > 5\">\n              <input type=\"range\" inline title=\"Card 7 Logo Count\" name=\"items7\" min=\"0\" max=\"4\" value=\"3\" step=\"1\" condition=\"cardsCount > 6\">\n              <input type=\"range\" inline title=\"Card 8 Logo Count\" name=\"items8\" min=\"0\" max=\"4\" value=\"4\" step=\"1\" condition=\"cardsCount > 7\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n         <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <header>Background</header>\n        <input type=\"color\" title=\"Icon BG\" value=\"#ec1818\" name=\"iconBg\" selected>\n        <input type=\"color\" title=\"Number Color\" value=\"#ffffff\" name=\"numberColor\" selected>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@THEME_PATH@/components/_images/bg_3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Background Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        \n        <div class=\"row align-items-start justify-content-center\">\n            \n            <div class=\"col-lg-2 col-md-2 sidebar align-right\"> \n                <div class=\"items-list mbr-fonts-style\" mbr-